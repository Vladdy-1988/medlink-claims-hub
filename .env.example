# Environment Configuration for MedLink Claims Hub

# Application
NODE_ENV=production
PORT=5000

# Database (Required)
DATABASE_URL=postgresql://username:password@host:port/database

# Authentication (Required for Replit deployment)
REPLIT_DB_URL=
REPL_ID=
REPL_SLUG=

# Object Storage (Optional - falls back to local storage)
GOOGLE_CLOUD_PROJECT_ID=
GOOGLE_CLOUD_STORAGE_BUCKET=
GOOGLE_APPLICATION_CREDENTIALS=
STORAGE_DIR=./uploads

# Session Security (Auto-generated if not provided)
SESSION_SECRET=

# API Keys (Optional)
TELUS_API_KEY=
CDANET_API_KEY=

# Monitoring (Optional)
ENABLE_METRICS=true
LOG_LEVEL=info

# Security (Optional)
CORS_ORIGIN=
ALLOWED_HOSTS=