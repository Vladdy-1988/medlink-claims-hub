# Database Configuration
DATABASE_URL="postgresql://username:password@localhost:5432/medlink_claims"
PGHOST="localhost"
PGPORT="5432"
PGUSER="username"
PGPASSWORD="password"
PGDATABASE="medlink_claims"

# Authentication
SESSION_SECRET="your-secret-session-key-here"
REPL_ID="your-replit-repl-id"
ISSUER_URL="https://replit.com/oidc"

# Replit Environment
REPLIT_DOMAINS="your-replit-domain.replit.app"

# Object Storage
DEFAULT_OBJECT_STORAGE_BUCKET_ID="your-bucket-id"
PUBLIC_OBJECT_SEARCH_PATHS="/bucket-name/public,/bucket-name/assets"
PRIVATE_OBJECT_DIR="/bucket-name/private"

# Application
NODE_ENV="development"
PORT="5000"

# Feature Flags
ENABLE_AUDIT_LOGGING="true"
ENABLE_SCHEDULER="true"
ENABLE_OFFLINE_SYNC="true"